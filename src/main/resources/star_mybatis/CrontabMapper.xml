<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hujian.star.mapper.CrontabMapper">

    <insert id="setCronLog" parameterType="java.lang.String">
        INSERT INTO `CRON_LOG`(`DATA`,`DATE`)
        VALUES (#{log},curdate());
    </insert>

</mapper>